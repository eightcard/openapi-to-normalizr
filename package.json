{
  "name": "openapi-to-normalizr",
  "description": "generator for normalizr",
  "version": "0.4.4",
  "author": {
    "name": "Eight",
    "url": "https://github.com/eightcard/openapi-to-normalizr"
  },
  "bin": {
    "openapi2schemas": "./bin/generateschemas"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "10.1.0",
    "commander": "11.0.0",
    "js-yaml": "4.1.0",
    "lodash": "4.17.21",
    "mkdirp": "1.0.4",
    "mustache": "4.2.0",
    "swagger-client": "3.18.0"
  },
  "devDependencies": {
    "@babel/cli": "7.22.15",
    "@babel/core": "7.22.20",
    "@babel/preset-env": "7.22.20",
    "@babel/preset-typescript": "7.22.15",
    "@babel/register": "7.22.15",
    "@stoplight/spectral-cli": "6.10.1",
    "@types/jest": "29.5.4",
    "@types/js-yaml": "4.0.6",
    "@types/json-schema": "7.0.12",
    "@types/lodash": "4.14.198",
    "@types/mkdirp": "1.0.2",
    "@types/mustache": "4.2.2",
    "@types/prop-types": "15.7.5",
    "@types/react-immutable-proptypes": "2.1.0",
    "@types/redux-actions": "2.6.2",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "babel-jest": "29.6.4",
    "babel-loader": "9.1.3",
    "babel-polyfill": "6.26.0",
    "babel-preset-power-assert": "3.0.0",
    "core-js": "3.32.2",
    "eslint": "8.49.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-prettier": "5.0.0",
    "husky": "8.0.3",
    "immutable": "4.2.1",
    "jest": "29.6.4",
    "json-server": "0.17.3",
    "lint-staged": "13.3.0",
    "nock": "13.3.3",
    "normalizr": "3.6.2",
    "npm-run-all": "4.1.5",
    "openapi-types": "12.1.3",
    "power-assert": "1.6.1",
    "prettier": "3.0.3",
    "prop-types": "15.8.1",
    "react-immutable-proptypes": "2.2.0",
    "redux": "4.2.1",
    "regenerator-runtime": "0.14.0",
    "rimraf": "4.4.1",
    "typescript": "5.2.2"
  },
  "peerDependencies": {
    "immutable": "^4.0.0-rc",
    "normalizr": "^3.2.0",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.0"
  },
  "resolutions": {
    "json-schema": ">=0.4.0",
    "jsonpointer": ">=5.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --allow-empty"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "files": [
    "config",
    "src",
    "dist",
    "templates",
    "examples",
    "bin",
    "README.md",
    "package.json"
  ],
  "keywords": [
    "generator",
    "open api",
    "swagger"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "eightcard/openapi-to-normalizr",
  "scripts": {
    "build:dist": "run-p build:dist:*",
    "build:dist:lib": "babel -d dist --env-name=development --extensions '.ts' src/lib/**/*[^test].ts",
    "build:dist:tools": "babel -d dist/compiled --env-name=development --extensions '.ts' src/tools/**/*.ts",
    "dev": "run-p dev:*",
    "dev:lib": "yarn build:dist:lib -w",
    "dev:tools": "yarn build:dist:tools -w",
    "check:typescript": "tsc --noEmit",
    "check:prettier": "prettier --check src example/src",
    "eslint": "eslint src bin/*",
    "mock": "cp example/initial_db.json example/db.json;json-server --watch example/db.json --port 10010",
    "test": "NODE_OPTIONS='--no-experimental-fetch' npm-run-all lint jest",
    "jest": "jest src/lib spec",
    "lint": "run-p check:* eslint speclint",
    "speclint": "spectral lint example/timeline.v3.yml spec/**/*.yml",
    "example:api": "./example/serveViewer",
    "example:build": "./bin/generateschemas --config example/config/config.schemas.js example/timeline.v3.yml",
    "example:run": "cd example;yarn && yarn start",
    "example": "npm-run-all example:build --parallel mock example:run"
  }
}
