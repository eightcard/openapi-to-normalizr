{
  "name": "openapi-to-normalizr",
  "description": "generator for normalizr",
  "version": "0.1.5",
  "author": {
    "name": "Eight",
    "url": "https://github.com/eightcard/openapi-to-normalizr"
  },
  "bin": {
    "openapi2schemas": "./bin/generateschemas"
  },
  "engines": {
    "node": ">=12.14.0"
  },
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "9.0.9",
    "commander": "8.1.0",
    "js-yaml": "4.1.0",
    "lodash": "4.17.21",
    "mkdirp": "1.0.4",
    "mustache": "4.2.0",
    "swagger-client": "3.15.0"
  },
  "devDependencies": {
    "@babel/cli": "7.14.8",
    "@babel/core": "7.15.0",
    "@babel/preset-env": "7.15.0",
    "@babel/preset-typescript": "7.15.0",
    "@babel/register": "7.15.3",
    "@types/jest": "27.0.1",
    "@types/js-yaml": "4.0.3",
    "@types/json-schema": "7.0.9",
    "@types/lodash": "4.14.172",
    "@types/mkdirp": "1.0.2",
    "@types/mustache": "4.1.2",
    "@types/prop-types": "15.7.4",
    "@types/react-immutable-proptypes": "2.1.0",
    "@types/redux-actions": "2.6.2",
    "@types/rimraf": "3.0.2",
    "@typescript-eslint/eslint-plugin": "4.29.1",
    "@typescript-eslint/parser": "4.29.1",
    "babel-jest": "27.0.6",
    "babel-loader": "8.2.2",
    "babel-polyfill": "6.26.0",
    "babel-preset-power-assert": "3.0.0",
    "core-js": "3.16.2",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "husky": "7.0.1",
    "immutable": "4.0.0-rc.14",
    "jest": "27.0.6",
    "json-server": "0.16.3",
    "lint-staged": "11.1.2",
    "nock": "13.1.1",
    "normalizr": "3.6.1",
    "npm-run-all": "4.1.5",
    "openapi-types": "9.1.0",
    "power-assert": "1.6.1",
    "prettier": "2.3.2",
    "prop-types": "15.7.2",
    "react-immutable-proptypes": "2.2.0",
    "redux": "4.1.1",
    "regenerator-runtime": "0.13.9",
    "rimraf": "3.0.2",
    "speccy": "0.11.0",
    "typescript": "4.3.5"
  },
  "peerDependencies": {
    "immutable": "^4.0.0-rc",
    "normalizr": "^3.2.0",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --allow-empty"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "files": [
    "config",
    "src",
    "dist",
    "templates",
    "examples",
    "bin",
    "README.md",
    "package.json"
  ],
  "keywords": [
    "generator",
    "open api",
    "swagger"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "eightcard/openapi-to-normalizr",
  "scripts": {
    "build:dist": "run-p build:dist:*",
    "build:dist:lib": "babel -d dist --env-name=development --extensions '.ts' src/lib/**/*[^test].ts",
    "build:dist:tools": "babel -d dist/compiled --env-name=development --extensions '.ts' src/tools/**/*.ts",
    "check:typescript": "tsc --noEmit",
    "check:prettier": "prettier --check src example/src",
    "eslint": "eslint src bin/*",
    "mock": "cp example/initial_db.json example/db.json;json-server --watch example/db.json --port 10010",
    "test": "npm-run-all lint jest",
    "jest": "jest src/lib spec",
    "lint": "run-p check:* eslint speclint",
    "speclint": "find example/petstore.v3.yml spec -name '*.yml' | xargs -I{} yarn speccy {}",
    "speccy": "speccy lint -s 'operation-operationId' ",
    "example:api": "./example/serveViewer",
    "example:build": "./bin/generateschemas --config example/config/config.schemas.js example/timeline.v3.yml",
    "example:run": "cd example;yarn && yarn start",
    "example": "npm-run-all example:build --parallel mock example:run"
  }
}
